<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 为白色 -->
    <solid android:color="@color/halfgray" />

    <!-- 设置几何图形四个角的弧度 -->
    <corners android:radius="10dp"/>

    <!-- 设置几何形状绘制边框 灰色边框 -->
    <stroke android:color="@color/white"
            android:width="1dp"/>

</shape>